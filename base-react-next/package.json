{
  "name": "trtc-js-samples-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "lint": "eslint ./src",
    "fix": "eslint ./src --fix",
    "wrap": "npx a18n wrap src/pages --write && npx a18n wrap src/components --write && npx a18n wrap src/config --write",
    "extract": "npx a18n extract src ./src/locales --locales zh-CN,en",
    "excel": "json2excel ./src/locales/zh-CN.json ./src/locales/en.json ./src/locales/translation.xlsx",
    "json": "json2excel -r ./src/locales/translation.xlsx",
    "translate-replace": "node ./script/index.js"
  },
  "dependencies": {
    "@material-ui/core": "4.11.4",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.58",
    "@zeit/next-css": "^1.0.1",
    "a18n": "^1.5.4",
    "axios": "^0.21.1",
    "babel-plugin-styled-components": "^1.12.0",
    "clsx": "^1.1.1",
    "is-mobile": "^3.0.0",
    "js-cookie": "^2.2.1",
    "json-excel-cli": "^1.0.17",
    "next": "10.2.0",
    "prop-types": "^15.7.2",
    "qrcode.react": "^1.0.1",
    "qs": "^6.10.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "^11.8.15",
    "rtc-ai-denoiser": "^1.1.3",
    "rtc-audio-mixer": "0.0.1",
    "rtc-beauty-plugin": "^0.0.7",
    "rtc-device-detector-react": "^1.0.6",
    "sass": "^1.32.12",
    "trtc-js-sdk": "^4.11.7",
    "ua-parser-js": "^1.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.21.0",
    "eslint-config-tencent": "^1.0.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.24.0",
    "line-reader": "^0.4.0",
    "prettier": "^2.7.1",
    "typescript": "~4.5.5"
  }
}
